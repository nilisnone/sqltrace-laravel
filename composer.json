{
    "name": "nilisnone/sqltrace-laravel",
    "type": "project",
    "description": "The Laravel Framework's sql trace",
    "keywords": [
        "framework",
        "laravel",
        "sqltrace",
        "sqldebug",
        "debuger"
    ],
    "license": "MIT",
    "require": {
        "php": "php^7.3|^7.4|^8.2",
        "symfony/polyfill-uuid": "^1.13"
    },
    "suggest": {
        "monolog/monolog": "Allow log with context"
    },
    "autoload": {
        "psr-4": {
            "SQLTrace\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "SQLTrace\\ServiceProvider"
            ],
            "aliases": {
                "SQLTrace": "SQLTrace\\Facade"
            }
        }
    }
}
